{
  "name": "tsushobot",
  "version": "1.1.1",
  "description": "Tsusho Discord bot",
  "main": "index.js",
  "scripts": {
    "build:src": "tsc -b src",
    "db:migrate": "npx sequelize-cli db:migrate",
    "env:new": "node -p \"require('fs').copyFile('.env.copy', '.env', (err)=>{if(err){throw err}})\"",
    "test": "jest --forceExit --coverage --verbose",
    "test:legacy": "mocha test",
    "start": "concurrently \"npm run build:src\" \"node dist/index.js\" --max-processes 1",
    "start:nobuild": "node dist/index.js",
    "postinstall": "concurrently \"npm run db:migrate\" \"npm run build:src\" \"npm run env:new\" --max-processes 1",
    "version": "node -p \"require('./package').version\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VuongRon/TsushoBot.git"
  },
  "author": "Tsusho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VuongRon/TsushoBot/issues"
  },
  "homepage": "https://github.com/VuongRon/TsushoBot#readme",
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "axios": "^0.21.1",
    "discord.js": "^13.0.1",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "mime-types": "^2.1.27",
    "multivariate-normal": "^0.1.2",
    "numbers": "^0.7.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/jest": "^26.0.20",
    "@types/multivariate-normal": "^0.1.0",
    "@types/validator": "^13.1.3",
    "chai": "^4.2.0",
    "concurrently": "^6.1.0",
    "jest": "^26.6.3",
    "sinon": "^9.2.2",
    "sinon-chai": "^3.5.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
